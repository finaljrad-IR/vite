{
  "name": "rest-express-merged",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "homepage": "https://vite.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitejs/vite.git"
  },
  "keywords": [
    "frontend",
    "hmr",
    "dev-server",
    "build-tool",
    "vite"
  ],
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "template:preinstall": "npx only-allow pnpm",
    "template:postinstall": "simple-git-hooks",
    "template:format": "prettier --write --cache .",
    "template:lint": "eslint --cache .",
    "template:typecheck": "tsc -p scripts && pnpm -r --parallel run typecheck",
    "template:test": "pnpm test-unit && pnpm test-serve && pnpm test-build",
    "template:test-serve": "vitest run -c vitest.config.e2e.ts",
    "template:test-build": "VITE_TEST_BUILD=1 vitest run -c vitest.config.e2e.ts",
    "template:test-unit": "vitest run",
    "template:test-docs": "pnpm run docs-build",
    "template:debug-serve": "VITE_DEBUG_SERVE=1 vitest run -c vitest.config.e2e.ts",
    "template:debug-build": "VITE_TEST_BUILD=1 VITE_PRESERVE_BUILD_ARTIFACTS=1 vitest run -c vitest.config.e2e.ts",
    "template:docs": "pnpm --filter=docs run docs",
    "template:docs-build": "pnpm --filter=docs run docs-build",
    "template:docs-serve": "pnpm --filter=docs run docs-serve",
    "template:build": "pnpm -r --filter='./packages/*' run build",
    "template:dev": "pnpm -r --parallel --filter='./packages/*' run dev",
    "template:release": "tsx scripts/release.ts",
    "template:ci-publish": "tsx scripts/publishCI.ts",
    "template:ci-docs": "pnpm build && pnpm docs-build"
  },
  "dependencies": {
