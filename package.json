    "@types/express": "4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/less": "^3.0.8",
    "@types/node": "^22.17.1",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/picomatch": "^4.0.2",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/stylus": "^0.48.43",
    "@types/ws": "^8.18.1",
